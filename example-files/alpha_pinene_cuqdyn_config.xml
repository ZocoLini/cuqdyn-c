<?xml version="1.0" encoding="UTF-8" ?>

<cuqdyn-config>
    <tolerances>
        <rtol>10</rtol>
        <atol>10, 10, 10, 10, 10</atol>
    </tolerances>
    <ode_expr y_count="5" p_count="5">
        -(p1 + p2) * y1
        p1 * y1
        p2 * y1 - (p3 + p4) * y3 + p5 * y5
        p3 * y3
        p4 * y3 - p5 * y5
    </ode_expr>
</cuqdyn-config>
